#ifndef OSGNVPR_VERSION
#define OSGNVPR_VERSION

#include <osgNVPR/Export>

extern "C" {

#define OSGNVPR_MAJOR_VERSION 0
#define OSGNVPR_MINOR_VERSION 1
#define OSGNVPR_PATCH_VERSION 0

#define OSGNVPR_MIN_VERSION_REQUIRED(MAJOR, MINOR, PATCH) ((OSGNVPR_MAJOR_VERSION>MAJOR) || (OSGNVPR_MAJOR_VERSION==MAJOR && (OSGNVPR_MINOR_VERSION>MINOR || (OSGNVPR_MINOR_VERSION==MINOR && OSGNVPR_PATCH_VERSION>=PATCH))))
#define OSGNVPR_VERSION_LESS_THAN(MAJOR, MINOR, PATCH) ((OSGNVPR_MAJOR_VERSION<MAJOR) || (OSGNVPR_MAJOR_VERSION==MAJOR && (OSGNVPR_MINOR_VERSION<MINOR || (OSGNVPR_MINOR_VERSION==MINOR && OSGNVPR_PATCH_VERSION<PATCH))))
#define OSGNVPR_VERSION_LESS_OR_EQUAL(MAJOR, MINOR, PATCH) ((OSGNVPR_MAJOR_VERSION<MAJOR) || (OSGNVPR_MAJOR_VERSION==MAJOR && (OSGNVPR_MINOR_VERSION<MINOR || (OSGNVPR_MINOR_VERSION==MINOR && OSGNVPR_PATCH_VERSION<=PATCH))))
#define OSGNVPR_VERSION_GREATER_THAN(MAJOR, MINOR, PATCH) ((OSGNVPR_MAJOR_VERSION>MAJOR) || (OSGNVPR_MAJOR_VERSION==MAJOR && (OSGNVPR_MINOR_VERSION>MINOR || (OSGNVPR_MINOR_VERSION==MINOR && OSGNVPR_PATCH_VERSION>PATCH))))
#define OSGNVPR_VERSION_GREATER_OR_EQUAL(MAJOR, MINOR, PATCH) ((OSGNVPR_MAJOR_VERSION>MAJOR) || (OSGNVPR_MAJOR_VERSION==MAJOR && (OSGNVPR_MINOR_VERSION>MINOR || (OSGNVPR_MINOR_VERSION==MINOR && OSGNVPR_PATCH_VERSION>=PATCH))))

}

#endif

